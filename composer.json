{
    "name": "itk-dev/config-bundle",
    "description": "A Symfony bundle to handle app config",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikkel Ricky",
            "email": "rimi@aarhus.dk"
        }
    ],
    "require": {
        "php": "^8.2",
        "easycorp/easyadmin-bundle": "^4.8",
        "craue/config-bundle": "dev-feature/symfony-7"
    },
    "autoload": {
        "psr-4": {
            "ItkDev\\ConfigBundle\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.49"
    },
    "scripts": {
        "coding-standards-check/php-cs-fixer": [
            "php-cs-fixer fix --dry-run"
        ],
        "coding-standards-check": [
            "@coding-standards-check/php-cs-fixer"
        ],
        "coding-standards-apply/php-cs-fixer": [
            "php-cs-fixer fix"
        ],
        "coding-standards-apply": [
            "@coding-standards-apply/php-cs-fixer"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "repositories": {
        "craue/config-bundle": {
            "type": "vcs",
            "url": "https://github.com/rimi-itk/CraueConfigBundle"
        }
    }
}
